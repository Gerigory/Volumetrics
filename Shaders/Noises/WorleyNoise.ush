    float frequency = pow(2, firstOctave);
    float WorleyNoise[3];
    for (int i = firstOctave; i < firstOctave + 3; i++)
    {
        uvw += iTime * 0.04;
        WorleyNoise[i] = worley(uvw * frequency, CellNum) / frequency;
		frequency *= 2.0;
    }
	return clamp(float3(WorleyNoise[0], WorleyNoise[1], WorleyNoise[2]) * saturateness, 0.0, 1.0);
